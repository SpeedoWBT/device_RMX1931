<resources>
    <!-- Whether to show a custom view for FOD -->
    <bool name="config_needCustomFODView">true</bool>

    <!-- Default state for LiveDisplay -->
    <bool name="config_liveDisplayAvailable">true</bool>

    <!-- Whether the device supports Smart Pixels -->
    <bool name="config_enableSmartPixels">true</bool>

    <!-- If true, the display will be shifted around in ambient mode. -->
    <bool name="config_enableBurnInProtection">true</bool>

    <!-- Whether device has VOOC charging support -->
    <bool name="config_hasVoocCharger">true</bool>

    <!-- States whether device has a high refresh rate display -->
    <bool name="config_deviceHasHighRefreshRate">true</bool>

    <!-- Use only if devices have variable intensity support for haptics-->
    <bool name="config_vibration_supports_multiple_intensities">true</bool>
</resources>