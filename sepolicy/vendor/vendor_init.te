  tombstone_data_file
  nfc_data_file
}:dir { create_dir_perms relabelfrom };

allow vendor_init persist_file:lnk_file read;
allow vendor_init proc_touchpanel:file { write };
allow vendor_init keystore_data_file:file { getattr };
allow vendor_init gatekeeper_data_file:file { getattr };
allow vendor_init keychain_data_file:file { getattr };
allow vendor_init net_data_file:file { getattr };
allow vendor_init radio_data_file:file { getattr };
allow vendor_init network_watchlist_data_file:file { getattr };
allow vendor_init textclassifier_data_file:file { getattr };
allow vendor_init vpn_data_file:file { getattr };
allow vendor_init block_device:lnk_file setattr;

set_prop(vendor_init, freq_prop)
